{
  "name": "tourze/tls-cert-extensions",
  "description": "A PHP library for handling TLS certificate extensions, with a focus on Certificate Transparency (CT) support and Signed Certificate Timestamp (SCT) validation",
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-hash": "*",
    "ext-openssl": "*",
    "tourze/tls-common": "0.0.*",
    "tourze/tls-crypto-asymmetric": "0.0.*",
    "tourze/tls-x509-core": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\TLSCertExtensions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\TLSCertExtensions\\Tests\\": "tests/"
    }
  }
}